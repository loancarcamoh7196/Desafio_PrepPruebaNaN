<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>E-mail</th>
            <th>Position</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% @people_list.each do |w| %>
            <tr>
                <td><%= w.person.fullname %></td>
                <td><%= w.person.email %></td>
                <td><%= w.person.position %></td>
                <td><%= link_to 'X', project_workshop_path(w.project.id, w.id), method: :delete %></td>
            </tr>
        <% end %>
        
    </tbody>

</table>